function checkResult(dsIntegracao, shouldThrowError) {&#xd;
    var code = dsIntegracao.getValue(0, &quot;Codigo&quot;);&#xd;
    var msg = dsIntegracao.getValue(0, &quot;Mensagem&quot;);&#xd;
    &#xd;
    log.info(&apos;checkResult produto&apos;);&#xd;
